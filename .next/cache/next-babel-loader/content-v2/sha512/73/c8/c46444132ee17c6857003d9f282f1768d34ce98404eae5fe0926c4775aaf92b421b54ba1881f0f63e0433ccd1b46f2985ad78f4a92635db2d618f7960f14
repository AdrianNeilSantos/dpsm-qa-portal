{"ast":null,"code":"import axios from \"axios\";\nimport jwt from 'jsonwebtoken';\nexport default async function addEducation(data, token) {\n  let cookieData = jwt.decode(token);\n  let facultyId = cookieData.facultyId;\n\n  try {\n    if (token) {\n      try {\n        if (formData.get('endDate') == \"\") {\n          formData.delete('endDate');\n        }\n\n        formData.append('facultyId', facultyId);\n        const response = await axios({\n          method: 'POST',\n          url: 'http://localhost:3001/api/faculty/basic-info/add/education',\n          data: formData,\n          headers: {\n            'Content-Type': 'multipart/form-data',\n            Authorization: `Bearer ${token}`\n          }\n        }).then(function (response) {\n          //handle success\n          console.log(response);\n        }).catch(function (response) {\n          //handle error\n          console.log(response);\n        });\n      } catch (err) {\n        console.error(err);\n        return err;\n      }\n    }\n  } catch (err) {\n    console.error(err);\n    return err;\n  }\n}","map":{"version":3,"sources":["C:/Users/Aji Maglanque/Desktop/dpsm-qa-portal/services/faculty/basic-info/addEducation.js"],"names":["axios","jwt","addEducation","data","token","cookieData","decode","facultyId","formData","get","delete","append","response","method","url","headers","Authorization","then","console","log","catch","err","error"],"mappings":"AAAA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,GAAP,MAAgB,cAAhB;AAEA,eAAe,eAAeC,YAAf,CAA4BC,IAA5B,EAAkCC,KAAlC,EAAyC;AACvD,MAAIC,UAAU,GAAGJ,GAAG,CAACK,MAAJ,CAAWF,KAAX,CAAjB;AACG,MAAIG,SAAS,GAAGF,UAAU,CAACE,SAA3B;;AACH,MAAI;AACH,QAAGH,KAAH,EAAU;AACT,UAAI;AACH,YAAGI,QAAQ,CAACC,GAAT,CAAa,SAAb,KAA2B,EAA9B,EAAkC;AACjCD,UAAAA,QAAQ,CAACE,MAAT,CAAgB,SAAhB;AACA;;AACDF,QAAAA,QAAQ,CAACG,MAAT,CAAgB,WAAhB,EAA6BJ,SAA7B;AACA,cAAMK,QAAQ,GAAG,MAAMZ,KAAK,CAAC;AACzBa,UAAAA,MAAM,EAAE,MADiB;AAEzBC,UAAAA,GAAG,EAAE,4DAFoB;AAGzBX,UAAAA,IAAI,EAAEK,QAHmB;AAIzBO,UAAAA,OAAO,EAAE;AAAC