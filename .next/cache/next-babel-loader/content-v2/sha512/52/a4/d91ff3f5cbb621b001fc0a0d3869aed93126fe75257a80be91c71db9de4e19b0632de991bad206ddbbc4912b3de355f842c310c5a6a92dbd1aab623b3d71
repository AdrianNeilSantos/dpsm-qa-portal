{"ast":null,"code":"import axios from \"axios\";\nimport jwt from 'jsonwebtoken';\nexport default async function updateWorkExp(data, token) {\n  let cookieData = jwt.decode(token);\n  let facultyId = cookieData.facultyId;\n\n  try {\n    if (token) {\n      let url = 'http://localhost:3001/api/faculty/basic-info/' + facultyId;\n      let header = {\n        headers: {\n          'Authorization': 'Bearer ' + token\n        }\n      };\n      let bod = null;\n\n      if (`${data.endDate}` == \"\") {\n        bod = {\n          workExpId: `${data.workExpId}`,\n          employerName: `${data.employerName}`,\n          position: `${data.position}`,\n          startDate: `${data.startDate}`,\n          description: `${data.description}`\n        };\n      } else {\n        bod = {\n          workExpId: `${data.workExpId}`,\n          employerName: `${data.employerName}`,\n          position: `${data.position}`,\n          startDate: `${data.startDate}`,\n          endDate: `${data.endDate}`,\n          description: `${data.description}`\n        };\n      }\n\n      try {\n        const response = await axios.put(url + \"/work-exp\", bod, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n\n        if (response.data.success) {\n          console.log(response.data);\n          return response.data;\n        } else {\n          console.error(response.message);\n          return response.data;\n        }\n      } catch (err) {\n        console.error(err);\n        return err;\n      }\n    } else {\n      console.log(access.result.message);\n    }\n  } catch (err) {\n    console.error(err);\n    return err;\n  }\n}","map":{"version":3,"sources":["C:/Users/Aji Maglanque/Desktop/dpsm-qa-portal/services/faculty/basic-info/updateWorkExp.js"],"names":["axios","jwt","updateWorkExp","data","token","cookieData","decode","facultyId","url","header","headers","bod","endDate","workExpId","employerName","position","startDate","description","