{"ast":null,"code":"import axios from \"axios\";\nimport jwt from 'jsonwebtoken';\nexport default async function updateFaculty(data) {\n  let token = document.cookie;\n  let cookieData = jwt.decode(token);\n  let facultyId = cookieData.facultyId;\n\n  try {\n    if (token) {\n      let url = 'http://localhost:3001/api/faculty/basic-info/' + facultyId;\n      let header = {\n        headers: {\n          'Authorization': 'Bearer ' + token\n        }\n      };\n\n      try {\n        const response = await axios.put(url + \"/work-exp\", {\n          workExpId: `${data.workExpId}`,\n          employerName: `${data.employerName}`,\n          position: `${data.position}`,\n          startDate: `${data.startDate}`,\n          endDate: `${data.endDate}`,\n          description: `${data.description}`\n        }, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n\n        if (response.data.success) {\n          console.log(response.data);\n          return response.data;\n        } else {\n          console.error(response.message);\n          return response.data;\n        }\n      } catch (err) {\n        console.error(err);\n        return err;\n      }\n    } else {\n      console.log(access.result.message);\n    }\n  } catch (err) {\n    console.error(err);\n    return err;\n  }\n}","map":{"version":3,"sources":["C:/Users/Aji Maglanque/Desktop/dpsm-qa-portal/services/faculty/basic-info/updateWorkExp.js"],"names":["axios","jwt","updateFaculty","data","token","document","cookie","cookieData","decode","facultyId","url","header","headers","response","put","workExpId","employerName","position","startDate","endDate","description","Authorization","success","console","log","error","message","err","access","result"],"mappings":"AAAA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,GAAP,MAAgB,cAAhB;AAEA,eAAe,eAAeC,aAAf,CAA6BC,IAA7B,EAAmC;AACjD,MAAIC,KAAK,GAAGC,QAAQ,CAACC,MAArB;AACA,MAAIC,UAAU,GAAGN,GAAG,CAACO,MAAJ,CAAWJ,KAAX,CAAjB;AACG,MAAIK,SAAS,GA