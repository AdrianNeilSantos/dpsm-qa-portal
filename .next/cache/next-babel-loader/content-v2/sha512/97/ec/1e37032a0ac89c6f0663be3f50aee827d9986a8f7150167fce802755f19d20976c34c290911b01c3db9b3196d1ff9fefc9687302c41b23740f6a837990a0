{"ast":null,"code":"import axios from \"axios\";\nimport jwt from 'jsonwebtoken';\nexport default async function deleteEducation(data) {\n  try {\n    let token = document.cookie;\n    let cookieData = jwt.decode(token);\n    let facultyId = cookieData.facultyId;\n    console.log(facultyId);\n\n    if (token) {\n      let url = \"http://localhost:3001/api/faculty/basic-info/\" + facultyId + \"/education\";\n\n      try {\n        const response = await axios.delete(url, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          },\n          data: {\n            educInfoId: `${data}`\n          }\n        });\n\n        if (response.data.success) {\n          return response.data;\n        } else {\n          console.error(response.message);\n          return response.data;\n        }\n      } catch (err) {\n        console.error(err);\n        return err;\n      }\n    }\n  } catch (err) {\n    console.error(err);\n    return err;\n  }\n}","map":{"version":3,"sources":["C:/Users/Aji Maglanque/Desktop/dpsm-qa-portal/services/faculty/basic-info/deleteEducation.js"],"names":["axios","jwt","deleteEducation","data","token","document","cookie","cookieData","decode","facultyId","console","log","url","response","delete","headers","Authorization","educInfoId","success","error","message","err"],"mappings":"AAAA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,GAAP,MAAgB,cAAhB;AAEA,eAAe,eAAeC,eAAf,CAA+BC,IAA/B,EAAqC;AACnD,MAAI;AACH,QAAIC,KAAK,GAAGC,QAAQ,CAACC,MAArB;AACA,QAAIC,UAAU,GAAGN,GAAG,CAACO,MAAJ,CAAWJ,KAAX,CAAjB;AACG,QAAIK,SAAS,GAAGF,UAAU,CAACE,SAA3B;AACAC,IAAAA,OAAO,CAACC,GAAR,CAAYF,SAAZ;;AAEH,QAAGL,KAAH,EAAU;AACT,UAAIQ,GAAG,GAAG,kDAAkDH,SAAlD,GAA8D,YAAxE;;AACA,UAAI;AACH,cAAMI,QAAQ,GAAG,MAAMb,KAAK,CAACc,MAAN,CAAaF,GAAb,EAAkB;AACxCG,UAAAA,OAAO,EAAE;AACRC,YAAAA,aAAa,EAAG,UAASZ,KAAM;AADvB,WAD+B;AAIxCD,UAAAA,IAAI,EAAE;AACLc,YAAAA,UAAU,EAAG,GAAEd,IAAK;AADf;AAJkC,SAAlB,CAAvB;;AAQA,YAAIU,QAAQ,CAACV,IAAT,CAAce,OAAlB,EAA2B;AAC1B,iBAAOL,QAAQ,CAACV,IAAhB;AACA,SAFD,MAEO;AA